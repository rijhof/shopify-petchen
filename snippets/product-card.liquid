    <a href="{{ product.url }}" class="product-card__media-link">
        <div class="product-card__media">
            {% if product.featured_image %}
                {% assign image_alt = product.featured_image.alt | default: product.title %}
                {% render 'image',
                        image: product.featured_image,
                        aspect_ratio: aspect_ratio | default: '1-1',
                        alt: image_alt,
                        loading: loading | default: 'lazy',
                        sizes: '(min-width: 1280px) 400px, (min-width: 768px) 33vw, 100vw'
                %}
            {% else %}
                <div class="product-card__placeholder">
                    {{ 'product-1' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
            {% endif %}
        </div>
    </a>

    <div class="product-card__content">
        <h3 class="product-card__title">
            <a class="h5" href="{{ product.url }}">{{ product.title }}</a>
        </h3>

        {% if product.metafields.custom.haupt_usp != blank %}
            <p class="product-card__usp meta">
                {{ product.metafields.custom.haupt_usp }}
            </p>
        {% endif %}

        <div class="product-card__footer">
            <div class="product-card__price">
                {% if product.compare_at_price > product.price %}
                    <span class="product-card__price--compare">
                        {{ product.compare_at_price | money }}
                    </span>
                {% endif %}
                <span class="product-card__price--current">
                    {{ product.price | money }}
                </span>
            </div>

            {% if show_quick_add != false %}
                <div class="product-card__quick-add">
                    {% if product.available %}
